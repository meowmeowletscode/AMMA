<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    android:layout_width="match_parent"
    android:layout_height="wrap_content"
    android:padding="8dp"
    android:background="@android:color/white">

    <TextView
        android:id="@+id/textAssetName"
        android:layout_width="0dp"
        android:layout_height="wrap_content"
        android:text="Asset Name"
        android:textStyle="bold"
        android:ellipsize="end"
        android:maxLines="1"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintWidth_max="100dp"
        app:layout_constraintWidth_default="wrap"
        app:layout_constraintEnd_toStartOf="@+id/textBarcode" />

    <TextView
        android:id="@+id/textBarcode"
        android:layout_width="0dp"
        android:layout_height="wrap_content"
        android:text="Barcode"
        android:ellipsize="end"
        android:maxLines="1"
        app:layout_constraintStart_toEndOf="@+id/textAssetName"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintWidth_max="100dp"
        app:layout_constraintWidth_default="wrap"
        app:layout_constraintEnd_toStartOf="@+id/textQuantity" />

    <TextView
        android:id="@+id/textQuantity"
        android:layout_width="0dp"
        android:layout_height="wrap_content"
        android:text="Quantity"
        android:ellipsize="end"
        android:maxLines="1"
        app:layout_constraintStart_toEndOf="@+id/textBarcode"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintWidth_max="80dp"
        app:layout_constraintWidth_default="wrap"
        app:layout_constraintEnd_toStartOf="@+id/textDescription" />

    <TextView
        android:id="@+id/textDescription"
        android:layout_width="0dp"
        android:layout_height="wrap_content"
        android:text="Description"
        android:ellipsize="end"
        android:maxLines="2"
        app:layout_constraintStart_toEndOf="@+id/textQuantity"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintWidth_max="150dp"
        app:layout_constraintWidth_default="wrap"
        app:layout_constraintEnd_toStartOf="@+id/textLabel" />

    <TextView
        android:id="@+id/textLabel"
        android:layout_width="0dp"
        android:layout_height="wrap_content"
        android:text="Label"
        android:ellipsize="end"
        android:maxLines="1"
        app:layout_constraintStart_toEndOf="@+id/textDescription"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintWidth_max="80dp"
        app:layout_constraintWidth_default="wrap"
        app:layout_constraintEnd_toEndOf="parent" />

</androidx.constraintlayout.widget.ConstraintLayout>
